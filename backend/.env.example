# MongoDB Database
MONGODB_URI=mongodb://localhost:27017/chatapp

# JWT Secret (minimum 32 characters for security)
JWT_SECRET=your_super_secret_jwt_key_at_least_32_characters_long

# Cloudinary Configuration (for image/video uploads)
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key  
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Environment
NODE_ENV=development

# Server Port
PORT=5002

# Frontend URLs (for CORS configuration)
FRONTEND_URL=http://localhost:5173
VERCEL_DOMAIN=your-app.vercel.app
VERCEL_GIT_DOMAIN=your-git-branch.vercel.app
VERCEL_PREVIEW_DOMAIN=your-preview.vercel.app